{
    "name" : "apnet/layout-bundle",
    "description" : "Default layout for all sites",
    "type" : "symfony-bundle",
    "license" : "MIT",
    "require" : {
        "php" : ">=5.3.3",
        "symfony/framework-bundle" : "~2.3",
        "apnet/bootstrap" : "~3.1,<4.0",
        "apnet/jquery" : "~1.11,<2.0",
        "apnet/html5shiv" : "~3.7,<4.0"
    },
    "autoload" : {
        "psr-0" : { "Apnet\\" : "src/" }
    },
    "require-dev" : {
        "symfony/symfony" : "~2.3",
        "symfony/monolog-bundle" : "~2.3",
        "apnet/functional-test-bundle" : "~1.1,>=1.1.4",
        "phpunit/phpunit" : "~4.0",
        "squizlabs/php_codesniffer" : "~1.5",
        "apnet/coding-standard" : "~1.5",
        "covex-nn/twig-callable-bridge-bundle" : "~1.0",
        "knplabs/knp-markdown-bundle" : "~1.3"
    },
    "extra" : {
        "symfony-app-dir" : "app",
        "symfony-web-dir" : "web",
        "branch-alias" : {
            "dev-master" : "3.x-dev"
        }
    },
    "config" : {
        "bin-dir" : "bin",
        "component-dir" : "web/components"
    }
}

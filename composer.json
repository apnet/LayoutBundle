{
    "name" : "apnet/layout-bundle",
    "description" : "Default layout for all sites",
    "type" : "symfony-bundle",
    "license" : "MIT",
    "require" : {
        "php" : ">=5.3.3",
        "symfony/framework-bundle" : "~2.3|~3.0",
        "apnet/bootstrap" : "~3.3",
        "apnet/jquery" : "~2.2",
        "apnet/html5shiv" : "~3.7"
    },
    "require-dev" : {
        "symfony/symfony" : "~2.3|~3.0",
        "symfony/monolog-bundle" : "~2.3",
        "apnet/functional-test-bundle" : "~1.2",
        "phpunit/phpunit" : "~4.0",
        "apnet/coding-standard" : "~2.0",
        "covex-nn/twig-callable-bridge-bundle" : "~1.1",
        "knplabs/knp-markdown-bundle" : "~1.3"
    },
    "autoload" : {
        "psr-0" : { "Apnet\\" : "src/" }
    },
    "extra" : {
        "symfony-app-dir" : "app",
        "symfony-web-dir" : "web",
        "branch-alias" : {
            "dev-master" : "3.x-dev"
        }
    },
    "config" : {
        "bin-dir" : "bin"
    }
}
